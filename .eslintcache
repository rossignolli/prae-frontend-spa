, useState } from 'react';\nimport 'react-nice-dates/build/style.css';\nimport Button from '../../Button';\nimport Header from '../../Header';\n\nimport AvatarEditor from 'react-avatar-editor';\nimport manProfile from '../../../assets/temp_assets/man-profile.png';\nimport InputTextField from '../../TextField';\nimport { useAuth } from '../../../hooks/AuthContext';\nimport InputImageFile from '../../ImageField';\nimport api from '../../../services/api';\nimport { ThreeDots } from 'react-loading-icons';\n\ninterface NewTranctionModalProps {\n  isOpen: boolean;\n  onRequestCancel: () => void;\n  onRequestConfirmation: () => void;\n  title?: string;\n  description?: string;\n  type?: 'warning' | 'error' | 'sucess' | 'info';\n  buttons?: boolean;\n}\n\ninterface User {\n  id: string;\n  avatar: string;\n  name: string;\n  email: string;\n}\n\ninterface PreviewImage {\n  name: string;\n  url: string;\n}\n\nexport default function ProfileModal({ isOpen, onRequestCancel, onRequestConfirmation }: NewTranctionModalProps) {\n  const [editor, setEditor] = useState<AvatarEditor>();\n  const [name, setName] = useState('');\n  const [error, setError] = useState(false);\n\n  const [images, setImages] = useState<File[]>([]);\n  const [fileBlob, setFileBlob] = useState<File>();\n  const [loading, setLoading] = useState(false);\n\n  const [previewImages, setPreviewImages] = useState<PreviewImage[]>([]);\n  const [scale, setScale] = useState(1.2);\n  const [rotation, setRotation] = useState(0);\n\n  const { user, updateUser } = useAuth();\n\n  useEffect(() => {\n    setName(user.name);\n  }, [user.name]);\n\n  async function handleSaveAvatar() {\n    const data = new FormData();\n\n    if (loading) {\n      return;\n    }\n\n    setLoading(true);\n\n    if (editor && images) {\n      const canvasScaled = editor.getImageScaledToCanvas();\n      const avatarUrl = canvasScaled.toDataURL();\n      const blobToSend = await fetch(avatarUrl).then(r => r.blob());\n      data.append('image', blobToSend);\n      data.append('id', user.id);\n      data.append('name', name);\n\n      const response = await api.patch<User>('users/avatar', data);\n\n      if (response.status !== 200) {\n        setError(true);\n        return;\n      }\n\n      const updatedUser = response.data;\n\n      updateUser({\n        id: updatedUser.id,\n        avatar: updatedUser.avatar,\n        name: updatedUser.name,\n        email: updatedUser.email,\n      });\n\n      setLoading(false);\n      setImages([]);\n      setPreviewImages([]);\n      setName(updatedUser.name);\n      onRequestConfirmation();\n      data.delete('name');\n      data.delete('id');\n      data.delete('image');\n\n      return;\n    }\n\n    data.append('id', user.id);\n    data.append('name', name);\n\n    const response = await api.patch<User>('users/avatar', data);\n\n    if (response.status !== 200) {\n      setError(true);\n      return;\n    }\n\n    const updatedUser = response.data;\n\n    updateUser({\n      id: updatedUser.id,\n      avatar: updatedUser.avatar,\n      name: updatedUser.name,\n      email: updatedUser.email,\n    });\n\n    setLoading(false);\n    setImages([]);\n    setPreviewImages([]);\n    setName(updatedUser.name);\n    onRequestConfirmation();\n    data.delete('name');\n    data.delete('id');\n    data.delete('image');\n  }\n\n  const setEditorRef = (editor: any) => {\n    editor = setEditor(editor);\n  };\n\n  function handleSelectImages(event: ChangeEvent<HTMLInputElement>) {\n    if (!event.target.files) {\n      return;\n    }\n    const selectedImages = Array.from(event.target.files);\n\n    event.target.value = '';\n\n    setImages(selectedImages);\n\n    const selectedImagesPreview = selectedImages.map(image => {\n      return { name: image.name, url: URL.createObjectURL(image) };\n    });\n\n    setPreviewImages(selectedImagesPreview);\n  }\n\n  function handleZoom(e: ChangeEvent<HTMLInputElement>) {\n    const scale = parseFloat(e.target.value);\n    setScale(scale);\n  }\n\n  function handleRotation(e: ChangeEvent<HTMLInputElement>) {\n    const rotation = parseFloat(e.target.value);\n    setRotation(rotation);\n  }\n\n  return (\n    <Modal isOpen={isOpen} overlayClassName=\"react-modal-overlay\" className=\"react-modal-content\" ariaHideApp={false}>\n      <S.ContainerModal>\n        <Header title=\"Editar Perfil\" description=\"\" />\n        <S.ModalContainer>\n          {images.length !== 0 && (\n            <>\n              <AvatarEditor\n                ref={setEditorRef}\n                image={previewImages[0].url}\n                width={200}\n                height={200}\n                rotate={rotation}\n                borderRadius={100}\n                border={30}\n                scale={scale}\n              />\n              <h1>Zoom</h1>\n              <input type=\"range\" min={0.1} max={4} step=\"0.01\" onChange={handleZoom} />\n              <h1>Rotação</h1>\n              <input type=\"range\" min={0} max={360} step=\"1\" onChange={handleRotation} />\n            </>\n          )}\n          <S.Userdiv>\n            {!previewImages.length && <img src={user.avatar ? user.avatar : manProfile} alt=\"\" />}\n            {!previewImages.length && <InputImageFile name=\"file\" label=\"Critico\" title={'Escolher arquivo'} onChange={handleSelectImages} />}\n          </S.Userdiv>\n          <InputTextField name=\"name\" label=\"Nome do perfil\" value={name} placeholder={''} onBlur={() => null} onChange={e => setName(e.target.value)} />\n        </S.ModalContainer>\n        <S.ButtonHolder>\n          <Button type=\"button\" customColor=\"#E1F5EC\" onClick={() => handleSaveAvatar()}>\n            {loading ? <ThreeDots style={{ width: '42px' }} /> : 'Salvar'}\n          </Button>\n          <Button\n            type=\"button\"\n            customColor=\"#F5E9EC\"\n            minimal\n            onClick={() => {\n              setLoading(false);\n              setImages([]);\n              setPreviewImages([]);\n              onRequestCancel();\n            }}>\n            Cancelar\n          </Button>\n        </S.ButtonHolder>\n      </S.ContainerModal>\n    </Modal>\n  );\n}\n","/Users/vigarani/Prae/prae-frontend-spa/src/components/Modals/ProfileModal/styles.ts",[],"/Users/vigarani/Prae/prae-frontend-spa/src/pages/Users/RecoverPasswordEmail/styles.ts",[],["419","420"],"/Users/vigarani/Prae/prae-frontend-spa/src/pages/Users/SignUp/index.tsx",["421","422"],"/Users/vigarani/Prae/prae-frontend-spa/src/pages/Users/SignIn/styles.ts",[],"/Users/vigarani/Prae/prae-frontend-spa/src/pages/Users/SignUp/styles.ts",[],"/Users/vigarani/Prae/prae-frontend-spa/src/pages/Users/RecoverPassword/styles.ts",[],"/Users/vigarani/Prae/prae-frontend-spa/src/pages/Users/RecoverPassword/index.tsx",["423","424","425","426","427"],"/Users/vigarani/Prae/prae-frontend-spa/src/pages/Users/RecoverPasswordEmail/index.tsx",[],"/Users/vigarani/Prae/prae-frontend-spa/src/pages/Users/SignIn/index.tsx",[],"/Users/vigarani/Prae/prae-frontend-spa/src/pages/Users/AccountConfirmation/index.tsx",[],"/Users/vigarani/Prae/prae-frontend-spa/src/pages/Users/ResendVerification/index.tsx",["428"],"/Users/vigarani/Prae/prae-frontend-spa/src/pages/Users/AccountConfirmation/styles.ts",[],"/Users/vigarani/Prae/prae-frontend-spa/src/pages/Jobs/JobsEdit/index.tsx",[],"/Users/vigarani/Prae/prae-frontend-spa/src/pages/Users/ResendVerification/styles.ts",[],"/Users/vigarani/Prae/prae-frontend-spa/src/components/EmptyStatus/index.tsx",["429"],"/Users/vigarani/Prae/prae-frontend-spa/src/components/CurrencyField/index.tsx",[],"/Users/vigarani/Prae/prae-frontend-spa/src/components/CurrencyField/styles.ts",[],{"ruleId":"430","replacedBy":"431"},{"ruleId":"432","replacedBy":"433"},{"ruleId":"430","replacedBy":"434"},{"ruleId":"432","replacedBy":"435"},{"ruleId":"430","replacedBy":"436"},{"ruleId":"432","replacedBy":"437"},{"ruleId":"430","replacedBy":"438"},{"ruleId":"432","replacedBy":"439"},{"ruleId":"440","severity":1,"message"